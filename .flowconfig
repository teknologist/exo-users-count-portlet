[include]

[libs]

[options]
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable
# Absolute path support:
# e.g. "/collections/todos"
module.name_mapper='^\/\(.*\)$' -> '<PROJECT_ROOT>/\1'


[ignore]
